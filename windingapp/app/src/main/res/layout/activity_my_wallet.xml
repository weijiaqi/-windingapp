<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="app.winding.com.windingapp.ui.MyWalletActivity">

    <include layout="@layout/common_base" />

    <include layout="@layout/linetwo_layout" />

    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_alignParentTop="true"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_15"
            android:text="支付宝账号："
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/rlDe_binding"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView20"
            android:layout_alignBottom="@+id/textView20"
            android:layout_marginLeft="@dimen/dp_80"
            android:text="去绑定"
            android:textColor="@color/color_fd"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/updateaccount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/rlDe_binding"
            android:layout_alignBottom="@+id/rlDe_binding"
            android:layout_marginStart="22dp"
            android:layout_toEndOf="@+id/rlDe_binding"
            android:autoLink="all"
            android:text="更改账户"
            android:textColor="@color/color_fd"
            android:textSize="@dimen/sp_10"
            android:visibility="gone" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@mipmap/redbal">

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_21"
            android:text="可 提 现 账 户"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_15" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView21"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="@dimen/sp_18"
            android:text="满100元即可提现"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/withdrawal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView21"
            android:layout_below="@+id/textView21"
            android:layout_marginTop="@dimen/dp_75"
            android:text="￥0.00"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/withdrawal"
            android:layout_below="@+id/withdrawal"
            android:layout_marginTop="@dimen/dp_12"
            android:text="当前余额"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/Cash_withdrawal"
            android:layout_width="@dimen/dp_65"
            android:layout_height="@dimen/dp_25"
            android:layout_alignBaseline="@+id/textView23"
            android:layout_alignBottom="@+id/textView23"
            android:layout_alignEnd="@+id/textView24"
            android:background="@drawable/bg_fborder"
            android:gravity="center"
            android:text="提现"
            android:textColor="@color/color_f8" />
    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@mipmap/frozenbak">

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_21"
            android:text="冻 结 账 户"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_15" />


        <TextView
            android:id="@+id/FrozenBlance"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView26"
            android:layout_below="@+id/textView26"
            android:layout_marginTop="@dimen/dp_75"
            android:text="￥0.00"
            android:textColor="@color/white" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/FrozenBlance"
            android:layout_below="@+id/FrozenBlance"
            android:layout_marginTop="@dimen/dp_12"
            android:gravity="center"
            android:text="当前余额"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_10" />
    </RelativeLayout>

    <TextView
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:lineSpacingExtra="@dimen/dp_5"
        android:text=""
        android:layout_marginTop="@dimen/dp_20" />
</LinearLayout>

